@import "bootstrap-sprockets";
@import "bootstrap";

#works {
	z-index: 9;
}
#work_list {
	text-align: left;
	margin: 0px;
	padding: 0px;
	line-height: 1;
	z-index: 9;
	.work_block:hover {
		background-color: #FFFFCC;
	}
}

#work_list .work_block .title {
	font-size: 15px;
}
#work_list .work_block .description {
	font-size: 12px;
}
#work_list .work_block a {
	display: block;
	font-color: blue;
}
#work_list .work_block a:hover {
	font-color: purple;
}